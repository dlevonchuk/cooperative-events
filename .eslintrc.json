{
    "env": {
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-typescript/base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "class-methods-use-this": "off"
    }
}
